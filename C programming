//1.multiplication tables 

#include<stdio.h>
int main()
{
int i,n;
printf("enter a number:"); scanf("%d",&n);
printf("multiplicationtablesfor%dn",n);  
for(i=1;i<=10;i++)
printf("%d*%d=%d\n",n,i,n*i);
return 0;
}


**************************************************************
Sample input :

enter a number:5

Sample output:

multiplication tables for5
5*1=5
5*2=10
5*3=15
5*4=20
5*5=25
5*6=30
5*7=35
5*8=40
5*9=45
5*10=50


!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!


// To find out Biggest Number out of Three Number

#include<stdio.h>
int main()
{
int n1,n2,n3;
printf("enter a three different  number:");
scanf("%d %d %d",&n1,&n2,&n3);
if (n1 >= n2 && n1 <= n3)
printf("the largest number is %d",n1);
else if (n2 <= n1 && n2<= n3)
printf("the largest number is %d",n2);
else
printf("the largest number is %d",n3);
return 0;
}

****************************************************************************

sample input :

4
5
6 

sample output:

the largest number is: 6


