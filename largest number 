// To find out Biggest Number out of Three Number

#include<stdio.h>
int main()
{
int n1,n2,n3;
printf("enter a three different  number:");
scanf("%d %d %d",&n1,&n2,&n3);
if (n1 >= n2 && n1 <= n3)
printf("the largest number is %d",n1);
else if (n2 <= n1 && n2<= n3)
printf("the largest number is %d",n2);
else
printf("the largest number is %d",n3);
return 0;
}

****************************************************************************

sample input :

4
5
6 

sample output:

the largest number is: 6

!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!